{
  "name": "@loaders.gl/geopackage",
  "description": "GeoPackage data loaders",
  "version": "3.0.1",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visgl/loaders.gl"
  },
  "keywords": [
    "sql",
    "GeoPackage"
  ],
  "type": "module",
  "types": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "sideEffects": false,
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "scripts": {
    "pre-build": "npm run build-worker --env.dev && npm run build-bundle",
    "build-worker": "webpack --entry ./src/workers/geopackage-worker.ts --output ./dist/geopackage-worker.js --config ../../scripts/webpack/worker.js",
    "build-bundle": "webpack --display=minimal --config ../../scripts/webpack/bundle.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "@loaders.gl/gis": "3.0.1",
    "@loaders.gl/schema": "3.0.1",
    "@loaders.gl/wkt": "3.0.1",
    "@math.gl/proj4": "^3.5.1",
    "@types/sql.js": "^1.4.2",
    "sql.js": "^1.5.0"
  }
}
